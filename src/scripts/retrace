#!/bin/sh

usage() {
   echo "usage: $0 <trace-file>"
   echo "  -where <trace-file> is a .trace - file generated by thinj"
   exit 1 
}

if [ $# -ne 1 ]; then
    usage $0
fi

if [ ! -f $1 ]; then
    echo "No such file: $1"
    usage $0
fi

java -cp ~/workspace/thinj/bin -Dtrace.file=$1 thinj.Retracer
